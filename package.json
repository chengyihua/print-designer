{
  "name": "print-designer",
  "version": "1.0.5",
  "homepage": "https://chengyihua.github.io/print-designer",
  "description": "报表设计器 React 组件库",
  "main": "dist/print-designer.umd.js",
  "module": "dist/print-designer.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/print-designer.es.js",
      "require": "./dist/print-designer.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chengyihua/print-designer.git"
  },
  "keywords": [
    "react",
    "print",
    "designer",
    "report",
    "band",
    "报表设计器"
  ],
  "author": "程宜华",
  "license": "MIT",
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jsbarcode": "^3.12.1",
    "jspdf": "^4.0.0",
    "qrcode": "^1.5.4",
    "react-rnd": "^10.5.2"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:lib": "vite build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build:lib",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-rnd": "^7.4.4",
    "@vitejs/plugin-react": "^4.7.0",
    "ajv": "^8.17.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "vite": "^6.4.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "web-vitals": "^2.1.4"
  }
}
